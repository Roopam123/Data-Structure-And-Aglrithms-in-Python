    "def pi(s):\n",
    "    if len(s)==0 or len(s)==1:\n",
    "        return s\n",
    "    if s[0]=='p' and s[1]=='i':\n",
    "        funCall = pi(s[2:])\n",
    "        return '3.14'+funCall\n",
    "    else:\n",
    "        funCall = pi(s[1:])\n",
    "        return s[0]+funCall"
   ]
