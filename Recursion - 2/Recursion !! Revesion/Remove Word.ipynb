   "source": [
    "def remove(s,a):\n",
    "    # Base Case\n",
    "    if len(s)==0:\n",
    "        return s\n",
    "    # Function call \n",
    "    funCall = remove(s[1:],a)\n",
    "    if s[0]==a:\n",
    "        return funCall\n",
    "    else:\n",
    "        return s[0] + funCall"
   ]
